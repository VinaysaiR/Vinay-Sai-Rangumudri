    "result = ''\n",
    "\n",
    "q = int(input(\"Enter the value of digit to converetd into binary(is greaterthan 0) = \"))\n",
    "\n",
    "while q != 0 :\n",
    "    r = q % 2\n",
    "    result += str(r)\n",
    "    q //= 2\n",
    "    \n",
    "result = result[::-1]\n",
    "\n",
    "print('the binary equivalent of',q,\"is:\",result)"
